<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<view title="Job"
		type="edit" 
		xmlns="http://www.skyve.org/xml/view" 
		xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <form percentageHeight="100">
        <column pixelWidth="150"/>
        <column/>
        <row>
            <item>
                <default binding="startTime"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="endTime"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="displayName"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="percentComplete"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="status"/>
            </item>
        </row>
        <row>
            <item>
                <textArea binding="log" />
            </item>
        </row>
    </form>
    <actions>
        <defaults/>
    </actions>
</view>

