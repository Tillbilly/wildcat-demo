<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<view title="Data Maintenance"
		type="edit"
		xmlns="http://www.skyve.org/xml/view"
		xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <form>
        <column percentageWidth="20"/>
        <column/>
        <column/>
        <row>
        	<item>
        		<combo binding="modDocName"/>
       		</item>
       		<item>
       			<button actionName="DataMaintenanceExportAction" />
       		</item>
      	</row>
    </form>
    <actions>
        <defaults />
		<export className="DataMaintenanceExportAction" displayName="Export Data" inActionPanel="false" />
		<import className="DataMaintenanceImportAction" displayName="Import Data" />
    </actions>
</view>
